{
  "name": "pepeso",
  "description": "A framework for building AI agents with tool usage capabilities",
  "version": "1.0.1",
  "main": "dist/index.min.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.min.js"
    },
    "./tools": {
      "types": "./dist/tools/index.d.ts",
      "default": "./dist/tools/index.js"
    },
    "./models": {
      "types": "./dist/models/index.d.ts",
      "default": "./dist/models/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "tools": [
        "dist/tools/index.d.ts"
      ],
      "models": [
        "dist/models/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "minify": "terser dist/index.js --compress --mangle --output dist/index.min.js && terser dist/tools/index.js --compress --mangle --output dist/tools/index.js && terser dist/models/index.js --compress --mangle --output dist/models/index.js",
    "prepublishOnly": "npm run build && npm run minify"
  },
  "keywords": [
    "ai",
    "agent",
    "agentic",
    "framework",
    "tools",
    "openai",
    "pepeso"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^5.4.1",
    "dotenv": "^16.5.0",
    "openai": "^5.3.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-console-format": "^1.0.8"
  },
  "devDependencies": {
    "@types/node": "^24.0.1",
    "terser": "^5.42.0",
    "typescript": "^5.8.3"
  }
}
